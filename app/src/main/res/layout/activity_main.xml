<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/home_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        />
    <ScrollView
        android:id="@+id/my_scroollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="false"
        android:layout_marginTop="@dimen/h_150"
        android:orientation="vertical"
        android:scrollbars="none"
        android:layout_alignParentBottom="true"
        android:fillViewport="false">
        <RelativeLayout
            android:id="@+id/relativeLayout_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:gravity="center">
            <RelativeLayout
                android:id="@+id/relativeLayout_dissertation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:focusable="false">
                <ImageView
                    android:id="@+id/imageView_dissertation_duoji"
                    android:layout_width="@dimen/w_252"
                    android:layout_height="@dimen/h_120"
                    android:layout_marginLeft="@dimen/w_85"
                    android:layout_marginRight="@dimen/w_11"
                    android:layout_marginTop="@dimen/h_15"
                    android:paddingRight="@dimen/w_5"
                    android:paddingLeft="@dimen/w_5"
                    android:paddingTop="@dimen/h_5"
                    android:paddingBottom="@dimen/h_5"
                    android:focusable="true"
                    android:descendantFocusability="afterDescendants"
                    android:src="@drawable/duojiduoli" />

                <ImageView
                    android:id="@+id/imageView_dissertation_guliguli"
                    android:layout_width="@dimen/w_252"
                    android:layout_height="@dimen/h_120"
                    android:layout_below="@+id/imageView_dissertation_duoji"
                    android:layout_marginLeft="@dimen/w_85"
                    android:layout_marginRight="@dimen/w_11"
                    android:paddingRight="@dimen/w_5"
                    android:paddingLeft="@dimen/w_5"
                    android:paddingTop="@dimen/h_5"
                    android:paddingBottom="@dimen/h_5"
                    android:focusable="true"
                    android:src="@drawable/guliguli" />

                <ImageView
                    android:id="@+id/imageView_dissertation_qisigongzhu"
                    android:layout_width="@dimen/w_252"
                    android:layout_height="@dimen/h_120"
                    android:layout_below="@+id/imageView_dissertation_guliguli"
                    android:layout_marginLeft="@dimen/w_85"
                    android:layout_marginRight="@dimen/w_11"
                    android:paddingRight="@dimen/w_5"
                    android:paddingLeft="@dimen/w_5"
                    android:paddingTop="@dimen/h_5"
                    android:paddingBottom="@dimen/h_5"
                    android:focusable="true"
                    android:src="@drawable/qisigongzhu" />

                <ImageView
                    android:id="@+id/imageView_all"
                    android:layout_width="@dimen/w_252"
                    android:layout_height="@dimen/h_120"
                    android:layout_below="@+id/imageView_dissertation_qisigongzhu"
                    android:layout_marginLeft="@dimen/w_85"
                    android:layout_marginRight="@dimen/w_11"
                    android:layout_marginBottom="@dimen/h_13"
                    android:paddingRight="@dimen/w_5"
                    android:paddingLeft="@dimen/w_5"
                    android:paddingTop="@dimen/h_5"
                    android:paddingBottom="@dimen/h_5"
                    android:focusable="true"
                    android:src="@drawable/gedan_all" />

                <ImageView
                    android:id="@+id/imageView_recent_play"
                    android:layout_width="@dimen/w_274"
                    android:layout_height="@dimen/h_254"
                    android:layout_marginLeft="@dimen/w_74"
                    android:layout_marginBottom="@dimen/h_2"
                    android:layout_marginTop="@dimen/h_2"
                    android:focusable="true"
                    android:src="@drawable/recent_record"
                   android:layout_below="@+id/imageView_all"
                    android:paddingRight="@dimen/w_16"
                    android:paddingLeft="@dimen/w_16"
                    android:paddingTop="@dimen/h_16"
                    android:paddingBottom="@dimen/h_16"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/relativeLayout_homepage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/relativeLayout_dissertation"
                android:layout_toRightOf="@+id/relativeLayout_dissertation"
                android:focusable="false"
                android:gravity="center">

                <ImageView
                    android:id="@+id/imageView_2"
                    android:layout_width="@dimen/w_730"
                    android:layout_height="@dimen/h_346"
                    android:layout_marginLeft="@dimen/w_19"
                    android:layout_marginRight="@dimen/w_74"
                    android:layout_marginBottom="@dimen/h_6"
                    android:paddingRight="@dimen/w_16"
                    android:paddingLeft="@dimen/w_16"
                    android:paddingTop="@dimen/h_16"
                    android:paddingBottom="@dimen/h_16"
                    android:layout_toRightOf="@+id/imageView_mine_collection"
                    android:nextFocusUp="@+id/imageView_dissertation_duoji"
                    android:focusable="true"
                    android:src="@drawable/image_2" />

                <ImageView
                    android:id="@+id/imageView_3"
                    android:layout_width="@dimen/w_356"
                    android:layout_height="@dimen/h_408"
                    android:layout_below="@+id/imageView_2"
                    android:layout_marginBottom="@dimen/h_2"
                    android:layout_marginTop="@dimen/h_2"
                    android:layout_marginLeft="@dimen/w_18"
                    android:layout_marginRight="@dimen/w_18"
                    android:paddingRight="@dimen/w_16"
                    android:paddingLeft="@dimen/w_16"
                    android:paddingTop="@dimen/h_16"
                    android:paddingBottom="@dimen/h_16"
                    android:layout_toRightOf="@+id/imageView_mine_collection"
                    android:focusable="true"
                    android:src="@drawable/image_3" />

                <ImageView
                    android:id="@+id/imageView_4"
                    android:layout_width="@dimen/w_356"
                    android:layout_height="@dimen/h_408"
                    android:layout_below="@+id/imageView_2"
                    android:layout_marginRight="@dimen/w_74"
                    android:layout_marginTop="@dimen/h_2"
                    android:layout_marginBottom="@dimen/h_2"
                    android:paddingRight="@dimen/w_16"
                    android:paddingLeft="@dimen/w_16"
                    android:paddingTop="@dimen/h_16"
                    android:paddingBottom="@dimen/h_16"
                    android:layout_toRightOf="@+id/imageView_3"
                    android:focusable="true"
                    android:src="@drawable/image_4" />

                <ImageView
                    android:id="@+id/imageView_play_record"
                    android:layout_width="@dimen/w_356"
                    android:layout_height="@dimen/h_254"
                    android:layout_alignBottom="@+id/imageView_3"
                    android:layout_marginLeft="@dimen/w_16"
                    android:layout_marginTop="@dimen/h_2"
                    android:layout_marginBottom="@dimen/h_2"
                    android:paddingRight="@dimen/w_16"
                    android:paddingLeft="@dimen/w_16"
                    android:paddingTop="@dimen/h_16"
                    android:paddingBottom="@dimen/h_16"
                    android:focusable="true"
                    android:src="@drawable/history_record" />

                <ImageView
                    android:id="@+id/imageView_mine_collection"
                    android:layout_width="@dimen/w_356"
                    android:layout_height="@dimen/h_254"
                    android:layout_alignBottom="@+id/imageView_3"
                    android:layout_marginLeft="@dimen/w_18"
                    android:layout_marginBottom="@dimen/h_2"
                    android:paddingRight="@dimen/w_16"
                    android:paddingLeft="@dimen/w_16"
                    android:paddingTop="@dimen/h_16"
                    android:paddingBottom="@dimen/h_16"
                    android:layout_toRightOf="@+id/imageView_play_record"
                    android:focusable="true"
                    android:src="@drawable/collection" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/relativeLayout_imageView_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/relativeLayout_homepage"
                android:layout_toRightOf="@+id/relativeLayout_dissertation"
                android:focusable="false"
                >
                <ImageView
                    android:id="@+id/imageView_1"
                    android:layout_width="@dimen/w_742"
                    android:layout_height="@dimen/h_512"
                    android:layout_marginRight="@dimen/w_20"
                    android:layout_marginLeft="@dimen/w_12"
                    android:paddingBottom="@dimen/h_22"
                    android:paddingLeft="@dimen/w_22"
                    android:paddingRight="@dimen/w_22"
                    android:paddingTop="@dimen/h_22"
                    android:focusable="true"
                    android:scaleType="fitXY"
                    android:src="@drawable/image_1" />

            </RelativeLayout>
            <LinearLayout
                android:id="@+id/linearLayout_image"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relativeLayout_homepage"
                android:descendantFocusability="afterDescendants"
                android:focusable="false"
                android:orientation="vertical" />
        </RelativeLayout>
    </ScrollView>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:focusable="false">
            <RelativeLayout

                android:id="@+id/relativeLayout_set"
                android:layout_width="match_parent"
                android:layout_height="@dimen/h_150"
                android:focusable="false">

                <ImageView
                    android:id="@+id/imageView_logo"
                    android:layout_width="@dimen/w_242"
                    android:layout_height="@dimen/h_110"
                    android:layout_marginLeft="@dimen/w_90"
                    android:layout_centerVertical="true"
                    android:focusable="false"
                    android:src="@drawable/logo_biaozhi2" />

                <TextView
                    android:id="@+id/textView_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/imageView_network"
                    android:focusable="false"
                    android:text="00ï¼š00"
                    android:textColor="@color/time"
                    android:textSize="@dimen/w_50" />

                <ImageView
                    android:id="@+id/imageView_network"
                    android:layout_width="@dimen/w_120"
                    android:layout_height="@dimen/h_120"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/w_40"
                    android:layout_toLeftOf="@+id/imageView_setting_item_fuhao"
                    android:focusable="false" />

                <ImageView
                    android:id="@+id/imageView_setting_item_fuhao"
                    android:layout_width="@dimen/w_120"
                    android:layout_height="@dimen/h_120"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/w_40"
                    android:layout_marginRight="@dimen/w_90"
                    android:background="@drawable/set"
                    android:scaleType="center"
                    android:focusable="true" />
                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/imageView_logo"
                    android:visibility="gone"
                    android:background="@drawable/watermark_xiaomi"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>